{
  "name": "hero-marketplace-ui",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.9.9",
    "big-integer": "^1.6.52",
    "ethers": "^6.13.2",
    "express": "^4.19.2",
    "graphql": "^16.8.1",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.22.3",
    "react-toastify": "^10.0.5",
    "web3": "^4.11.1",
    "dotenv": "^16.0.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "package": "npm run build && pkg . -t node14-win-x64 -o dist/HONK-Marketplace.exe --public --compress GZip",
    "serve": "node server.js",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write src/**/*.{js,jsx,json,css,scss,md}",
    "check-format": "prettier --check src/**/*.{js,jsx,json,css,scss,md}",
    "check-all": "npm run check-format && npm run lint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@openzeppelin/contracts": "^5.0.2",
    "@openzeppelin/contracts-upgradeable": "^5.0.2",
    "@openzeppelin/truffle-upgrades": "^1.21.0",
    "@truffle/hdwallet-provider": "^2.1.15",
    "autoprefixer": "^10.4.18",
    "customize-cra": "^1.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.2",
    "pkg": "^5.8.1",
    "postcss": "^8.4.35",
    "prettier": "^3.3.3",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.4.1",
    "truffle-plugin-verify": "^0.6.7"
  },
  "bin": "server.js",
  "pkg": {
    "assets": [
      "build/**/*"
    ],
    "targets": [
      "node14-win-x64"
    ],
    "outputPath": "dist",
    "compress": "GZip"
  }
}
